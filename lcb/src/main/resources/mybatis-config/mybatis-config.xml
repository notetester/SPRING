		<typeAlias type="com.lcb404.command.VoteVO" alias="VoteVO"/>
		<typeAlias type="com.lcb404.command.ReserveVO" alias="ReserveVO"/>
		<typeAlias type="com.lcb404.command.TimeTableVO" alias="TimeTableVO"/>
		<typeAlias type="com.lcb404.command.UserVO" alias="UserVO"/>	
		<typeAlias type="com.lcb404.command.PopcornVO" alias="PopcornVO"/>
		<typeAlias type="com.lcb404.command.MovieVO" alias="MovieVO"/>		
	</typeAliases>
</configuration>
